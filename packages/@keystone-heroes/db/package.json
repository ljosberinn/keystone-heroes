{
  "name": "@keystone-heroes/db",
  "version": "1.0.0",
  "private": true,
  "main": "src",
  "scripts": {
    "generate": "prisma generate",
    "push": "prisma db push",
    "seed": "prisma db seed",
    "format": "prettier --write src",
    "lint": "set TIMING=1 && eslint src --cache",
    "test": "jest --passWithNoTests",
    "static-data": "ts-node -r tsconfig-paths/register src/createStaticData && eslint --fix ../web/src/staticData.ts && prettier --write ../web/src/staticData.ts"
  },
  "dependencies": {
    "@keystone-heroes/api": "1.0.0",
    "@keystone-heroes/env": "1.0.0",
    "@keystone-heroes/wcl": "1.0.0",
    "@prisma/client": "3.0.2",
    "dotenv": "10.0.0",
    "prisma": "3.0.2",
    "ts-node": "10.2.1"
  },
  "devDependencies": {
    "eslint": "7.32.0",
    "eslint-config-galex": "2.16.12",
    "jest": "27.2.0",
    "ts-jest": "27.0.5",
    "typescript": "4.4.3"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}

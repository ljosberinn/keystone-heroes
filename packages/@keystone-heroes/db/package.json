{
  "name": "@keystone-heroes/db",
  "version": "1.0.0",
  "private": true,
  "main": "src",
  "scripts": {
    "format": "prettier --write src",
    "generate": "prisma generate",
    "lint": "set TIMING=1 && eslint src --cache",
    "push": "prisma db push",
    "seed": "prisma db seed",
    "static-data": "ts-node -r tsconfig-paths/register src/createStaticData && eslint --fix ../web/src/staticData.ts && prettier --write ../web/src/staticData.ts",
    "sync-dbs": "ts-node -r tsconfig-paths/register src/syncProdDevStaticData",
    "test": "jest --passWithNoTests"
  },
  "dependencies": {
    "@keystone-heroes/api": "1.0.0",
    "@keystone-heroes/env": "1.0.0",
    "@keystone-heroes/wcl": "1.0.0",
    "@prisma/client": "3.4.2",
    "dotenv": "10.0.0",
    "prisma": "3.4.2",
    "ts-node": "10.4.0"
  },
  "devDependencies": {
    "eslint": "8.2.0",
    "eslint-config-galex": "3.3.3",
    "jest": "27.3.1",
    "typescript": "4.4.4"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}

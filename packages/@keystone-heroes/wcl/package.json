{
  "name": "@keystone-heroes/wcl",
  "version": "1.0.0",
  "private": true,
  "main": "src",
  "scripts": {
    "format": "prettier --write src",
    "generate": "graphql-codegen --config codegen.yml && prettier --write src/types.ts && prettier --write schema.json",
    "introspection": "ts-node -r tsconfig-paths/register src/queries/introspection.ts --project tsconfig.json",
    "lint": "set TIMING=1 && eslint src --cache",
    "test": "jest",
    "wcl-static-data": "ts-node -r tsconfig-paths/register src/queries/wcl-static-data.ts --project tsconfig.json"
  },
  "dependencies": {
    "@keystone-heroes/db": "1.0.0",
    "@keystone-heroes/env": "1.0.0",
    "graphql": "16.0.1",
    "graphql-request": "3.6.1",
    "graphql-tag": "2.12.6",
    "node-fetch": "2.6.1",
    "tsconfig-paths": "3.11.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.3.0",
    "@graphql-codegen/introspection": "2.1.0",
    "@graphql-codegen/typescript": "2.4.0",
    "@graphql-codegen/typescript-graphql-request": "4.3.0",
    "@graphql-codegen/typescript-operations": "2.2.0",
    "eslint": "8.2.0",
    "eslint-config-galex": "3.3.3",
    "jest": "27.3.1",
    "msw": "0.35.0",
    "prettier": "2.4.1",
    "ts-essentials": "9.0.0",
    "ts-node": "10.4.0",
    "typescript": "4.4.4"
  },
  "graphql": {
    "schema": "./schema.json"
  }
}

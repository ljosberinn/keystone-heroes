{
  "name": "@keystone-heroes/wcl",
  "version": "1.0.0",
  "private": true,
  "main": "src",
  "scripts": {
    "lint": "set TIMING=1 && eslint src --cache",
    "format": "prettier --write src",
    "introspection": "ts-node -r tsconfig-paths/register src/queries/introspection.ts --project tsconfig.json",
    "wcl-static-data": "ts-node -r tsconfig-paths/register src/queries/wcl-static-data.ts --project tsconfig.json",
    "generate": "graphql-codegen --config codegen.yml && prettier --write src/types.ts && prettier --write schema.json",
    "test": "jest"
  },
  "dependencies": {
    "@keystone-heroes/db": "1.0.0",
    "@keystone-heroes/env": "1.0.0",
    "graphql": "15.5.1",
    "graphql-request": "3.4.0",
    "graphql-tag": "2.12.5",
    "node-fetch": "2.6.1",
    "tsconfig-paths": "3.9.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.21.6",
    "@graphql-codegen/introspection": "1.18.2",
    "@graphql-codegen/typescript": "1.22.4",
    "@graphql-codegen/typescript-graphql-request": "3.2.4",
    "@graphql-codegen/typescript-operations": "1.18.3",
    "eslint": "7.30.0",
    "eslint-config-galex": "2.16.6",
    "jest": "27.0.6",
    "msw": "0.30.1",
    "prettier": "2.3.2",
    "ts-essentials": "7.0.2",
    "ts-jest": "27.0.3",
    "ts-node": "10.0.0",
    "typescript": "4.3.5"
  },
  "graphql": {
    "schema": "./schema.json"
  }
}
